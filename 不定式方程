#include <stdio.h>
//21、百钱买百鸡问题。
// 公鸡值钱五，母鸡值钱三，小鸡三值一，白钱买百鸡，问公鸡、母鸡小鸡各几只？
int main(void) {
    int x,y,z;
    for (x = 0;x<=14;x++) {
        if ((100-7*x)%4==0) {
            y =(100-7*x)/4 ;
            z = 100-x-y;
            printf("x=%d,y=%d,z=%d ",x,y,z);
            printf("\n");
        }
    }
}
